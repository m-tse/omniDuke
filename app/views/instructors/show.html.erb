<%= render partial:"partials/breadcrumbs", locals:{breadCrumbHash:{"Home"=>root_path, "Professors"=>instructors_path, @instructor.toString=>'#'}} %>
<h1><%= @instructor.toString %></h1>
<h4>Courses Taught:
<% for course in @instructor.courses %>
<%= link_to courseToCode(course), course_path(course) %>
<% end %>
</h4>
<h4>Subjects Taught: 
<% for subject in @instructor.subjects_by_count %>
<%= link_to subject.name, subject_path(subject) %>
<% end %>
</h4>
