<title>OmniDuke Schedulator</title>
<!--h1 id="scheduleName">
</h1-->
<!--%= @name %-->

<div id="omniContainer">
    <div id="schedulatorContainer">
        <% if @edit %>
            <%= render partial: "schedulator/edit" %>   
        <% else %>
            <%= render partial: "schedulator/index" %>   
        <% end %>
    </div>
    <div id="side-menu">
        <% if $sideState == "saved" %>
            <%= render partial: "schedulator/saved" %>
        <% elsif $sideState == "search" %>
            <%= render partial: "schedulator/search" %>
        <% else %> 
            <%= render partial: "schedulator/class-list" %>
        <% end %>
    </div>
</div>
