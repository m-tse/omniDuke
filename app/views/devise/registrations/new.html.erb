<h2 class="title_of_horizontal_form">Sign up</h2>

<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name), :html =>{:class=>"form-horizontal"}) do |f| %>
  <%= devise_error_messages!.inspect %>
  <%= render partial: "devise/email_password_form", locals:{f:f, submit_label:"Sign up"} %>
<% end %>


