<!-- partial for session selector, ie fall2012->spring2013.  Must Pass in a path variable for where this partial will redirect to -->




<div class="dropdown">
<a class="dropdown-toggle" data-toggle="dropdown" href="#">Current Session: <%= current_or_guest_user.session.name%></a>
<ul class="dropdown-menu" role="menu" aria-labelledby="dLabel">
  <% for session in Session.all %>  
  <li><%= link_to session.name, send(path, :currentSession => session.name)%></li>
  <% end %>
</ul>
</div>
