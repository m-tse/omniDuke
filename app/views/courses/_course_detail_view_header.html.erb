<!-- must have @course from parent view -->
<div class="page-header">
  <h1><%= @course.name %></h1>
  </div>

    <ul class="nav nav-pills">
      <% if navHighlight=="Review" %>
      <li>
        <% else %>
      <li class="active">
        <% end %>
        <%= link_to "Overview", course_path(@course.id) %>
      </li>
      <% if navHighlight!="Review" %>
      <li>
        <% else %>
        <li class="active">
          <% end %>
        <%= link_to "Reviews", course_review_path(@course.id) %>
      </li>
    </ul>
